<p id="notice"><%= notice %></p>

<h1>Listing Voucher Accounts</h1>

<table>
  <thead>
    <tr>
      <th>No</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voucher_accounts.each do |voucher_account| %>
      <tr>
        <td><%= voucher_account.name %></td>
        <td><%= link_to 'Show', voucher_account %></td>
        <td><%= link_to 'Edit', edit_voucher_account_path(voucher_account) %></td>
        <td><%= link_to 'Destroy', voucher_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voucher Account', new_voucher_account_path %>
<br><%= link_to 'Voucher Account Help', voucher_accounts_help_path %>

